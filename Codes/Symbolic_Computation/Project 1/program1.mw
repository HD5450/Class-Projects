<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 1" layout="Heading 1">Discriminant Matrix</Text-field></Title>
<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">Program</Text-field></Title>
<Group hide-input="false" labelreference="L62" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">DiscriminantMatrix := proc(f,v)
#===================================
# Input:
#   f : a Pham system in v
#   v : a list of variables
# Output: 
#   Q : the discriminant matrix of f
#===================================
local d,poly,omega,p,Q;
d:=[];
for poly in f do
   d:=[op(d),degree(poly)];
od;
omega:= MonomialBasis(d,v);
p:=nops(omega);

Q_ij := proc(i,j)
#===================================
# Input:
#   i : i position for omega
#   j : j position for omega
# Output: 
#   M : trace of matrix M
#===================================
   local g,h,M,k;
   # zero list of size p   g := [seq(0, k=1..p)];
   for h from 1 to p do
     g[h] := Remainder(omega[i]*omega[j]*omega[h],f,v);
   od;
   M:=0;
   # Trace calculation
   for h from 1 to p do
     M:= M + Coefficient(g[h],omega[h]);
   od;
   return M;
end:
Q:= Matrix(p,p, Q_ij);
  return Q;
end:</Text-field>
</Input>
<Input><Text-field prompt="&gt; " style="Maple Input" spaceabove="0" linebreak="space" rightmargin="0" linespacing="0.0" initial="0" firstindent="0" leftmargin="0" bulletsuffix="" alignment="left" bullet="none" spacebelow="0" pagebreak-before="false"></Text-field>
</Input>
