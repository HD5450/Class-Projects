<Section collapsed="false" isCollapsible="true" drawButton="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title><Text-field style="Heading 2" layout="Heading 2">Program</Text-field></Title>
<Group labelreference="L12" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">Signature := proc(M)
#===================================
# Input:
#   M : a real symmetric matrix
# Output:
#   S : the signature of M
#===================================
  local m,C,mp,mz,mn,S;
  m  := RowDimension(M);
  C  := CharacteristicPolynomial(M,lambda);
  mp := SignVariationCount(C,lambda);
  mz := ldegree(C,lambda);
  mn := m - mp - mz;
  S  := mp - mn;
  return S; 
end:</Text-field>
</Input>
</Group>
</Section>
